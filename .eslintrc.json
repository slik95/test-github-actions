{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["import", "react"],
  "rules": {
    "import/extensions": ["error","ignorePackages"],
    "prettier/prettier": ["error"]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
